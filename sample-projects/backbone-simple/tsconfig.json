{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution":"node",
    "lib": [
      "es2018", "dom","es2018.promise"
    ],
    "outDir": "./dist",
    "rootDir": ".",
    // "strict": true,
    // "strictNullChecks": false,
    // "noImplicitAny": false,
    
    "noStrictGenericChecks": true,
    "strictBindCallApply": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,


    "noEmitHelpers": true,
    "importHelpers": true,
    "esModuleInterop": true,
    // "sourceMap": true,
    // "skipDefaultLibCheck": true,
    // "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "ReactLike.createElement",
    // "target": "es5",
    // "module": "none",
    
    // "declaration": true,
  },
  "include":["src"],
  "exclude": ["node_modules", "dist"],
}