{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6", "dom"
    ],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": false,
    "noStrictGenericChecks": true,
    "strictBindCallApply": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noEmitHelpers": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "ReactLike.createElement"
  },
  "include":["src"],
  // "exclude": ["node_modules", "dist"],
}