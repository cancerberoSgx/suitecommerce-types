{
    "name": "sc-types-frontend-extras",
    "version": "2018.1.0",
    "description": "SuiteCommerce TypeScript utilities like JSX/TSX support ",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "typings": "dist/src/index.d.ts",
    "scripts": {
        "build": "tsc",
        "build-watch": "tsc -w",
        "clean": "rm -rf dist",
        "test-js": "npm run clean && npm run build && node dist/spec",
        "test": "ts-node spec",
        "sc-tsc": "rm -rf compiled && npx sc-tsc --tsconfigFilePath ./tsconfig.json --outputFolder compiled"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cancerberoSgx/suitecommerce-types.git"
    },
    "keywords": [
        "SuiteCommerce",
        "TypeScript",
        "NetSuite",
        "devtools",
        "types",
        "JavaScript",
        "JSX",
        "TSX",
        "React"
    ],
    "author": "Sebastian Gurin",
    "license": "MIT",
    "homepage": "https://github.com/cancerberoSgx/suitecommerce-types",
    "dependencies": {
        "sc-types-frontend": "file:../sc-types-frontend"
    },
    "devDependencies": {
        "@types/jasmine": "^3.3.5",
        "@types/react": "^16.7.18",
        "@types/shelljs": "^0.8.1",
        "jasmine": "^3.3.1",
        "sc-tsc": "file:../ts-devtools",
        "shelljs": "^0.8.3",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2"
    }
}
