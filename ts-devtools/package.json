{
    "name": "sc-tsc",
    "version": "2018.1.0",
    "description": "SuiteCommerce Development Tools to develop new themes or extensions",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "typings": "dist/src/index.d.ts",
    "bin": "bin/sc-tsc",
    "scripts": {
        "build": "npm run clean && tsc && npm run copy",
        "build-watch": "tsc -w",
        "copy": "cp -r spec/support_js dist/spec/support",
        "clean": "rm -rf dist tmp",
        "generateHelp": "sh scripts/generateHelp.sh",
        "all": "npm run clean && npm run build && npm run test",
        "prepare": "npm run build",
        "test-run": "ts-node node_modules/jasmine/bin/jasmine",
        "test": "npm run test-install && npm run test-run",
        "test-install": "cd spec/fixtures/project1 && npm i && cd ../projectMine && npm i && cd ../using-sc-types-frontend-extras && npm i && cd ../tsxTest && npm i && cd ../../../../sample-projects/frontend-simple1 && npm i && cd ../jsx-view-tests && npm i && cd ../projectMine && npm i && cd ../tsx-test && npm i"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cancerberoSgx/suitecommerce-types.git"
    },
    "keywords": [
        "SuiteCommrce",
        "TypeScript",
        "Netsuite",
        "devtools",
        "types",
        "JavaScript"
    ],
    "author": "Sebastian Gurin",
    "license": "MIT",
    "homepage": "https://github.com/cancerberoSgx/suitecommerce-types",
    "dependencies": {
        "minimist": "^1.2.0",
        "misc-utils-of-mine": "file:../../misc-utils-of-mine",
        "shelljs": "^0.8.3",
        "ts-fix-all-errors": "file:../ts-fix-all-errors",
        "ts-simple-ast": "^20.0.0",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jasmine": "^3.3.5",
        "@types/minimist": "^1.2.0",
        "@types/node": "^10.12.18",
        "@types/shelljs": "^0.8.1",
        "generate-cli-api-help": "0.0.7",
        "jasmine": "^3.3.1",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2"
    }
}
